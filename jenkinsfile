
pipeline{
  agent any


   
  stages {

  stage('Install dependencies') {
      steps{
        
        script {
          dir('DevOps_Project_Front')
        {
                        // Run npm install in the front-end directory
        sh 'npm install'
    }
 
        }

         script {
          dir('DevOps_Project')
        {
                        // Run npm install in the front-end directory
        sh 'mvn clean install'
    }
 
        }
      }
    }


  } 

}

